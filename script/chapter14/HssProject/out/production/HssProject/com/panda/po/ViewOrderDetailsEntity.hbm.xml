<?xml version='1.0' encoding='utf-8'?>
<!DOCTYPE hibernate-mapping PUBLIC
    "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
    "http://www.hibernate.org/dtd/hibernate-mapping-3.0.dtd">
<hibernate-mapping>

    <class name="com.panda.po.ViewOrderDetailsEntity" table="VIEW_ORDER_DETAILS" schema="STUDY">
        <id name="id" column="ID"/>
        <property name="orderId" column="ORDER_ID"/>
        <property name="customerName" column="CUSTOMER_NAME"/>
        <property name="customerTel" column="CUSTOMER_TEL"/>
        <property name="orderDate" column="ORDER_DATE"/>
        <property name="productId" column="PRODUCT_ID"/>
        <property name="productName" column="PRODUCT_NAME"/>
        <property name="productType" column="PRODUCT_TYPE"/>
        <property name="productNum" column="PRODUCT_NUM"/>
        <property name="productPrice" column="PRODUCT_PRICE"/>
    </class>
</hibernate-mapping>